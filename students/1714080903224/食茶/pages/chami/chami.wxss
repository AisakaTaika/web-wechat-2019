 page {
    height: 100%;
  }

  .tab_container {
    width: 100%;
    height: 200%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
  }/*设置列表样式*/
  
  .nav-view-two {
    display: flex;
    width: 100%;
    flex-direction: row;
    align-items: center;
    justify-content: center;
    border: 1px solid #CDBA96;
    margin: 1px;
  }/*线*/

  .tab_content {
    width: 100%;
    display: flex;
    flex-direction: column;
     align-items: center;
    justify-content: center;
  }

  .nav_one {
    padding: 10px;
    width: 100%;
    display: flex;
    align-items: center;

  }/*设置展开图形*/

  .nav_text {
    flex: 1;
    display: flex;
    align-items: center;
    color: #CDBA96;
    font-size: 50rpx;
  }

  .icon-arrow:after {
    content: '';
    display: inline-block;
    align-items: center;
    justify-content: center;
    width: 7px;
    height: 7px;
    margin-right: 6rpx;
    border-width: 2px 2px 0 0;
    border-color: #6c934a;
    border-style: solid;
    transform: rotate(45deg);
  }/*展开列表图案*/

  .fx-zx
  {
  width: 200rpx;
  height: 200rpx;
  }
