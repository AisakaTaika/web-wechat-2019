.doraemon{position: relative;}
.doraemon .head{
  width: 320rpx;height: 300rpx;
  border-radius: 150rpx;
  background: #07bbee;
  background:-webkit-radial-gradient(right top,#fff 10%,#07bbee 20%,#10a6ce 75%,#000);
  background: radial-gradient(right top,#fff 10%,#07bbee 20%,#10a6ce 75%,#000);
  border: #555 2rpx solid;
  box-shadow: -5rpx 10rpx 15rpx rgba(0,0,0,0.45);
  position: relative;
}

.doraemon .face{position: relative; z-index: 2;}
.doraemon .face .white{
  border:#000 2rpx solid;
  width: 265rpx ;height: 195rpx;
  border-radius: 150rpx 150rpx;
  position: absolute;
  top: 75rpx;left: 25rpx;
  background: #fff;
  background: radial-gradient(right top,#fff 75%,#eee 80%,#999 90%,#444);
}

.doraemon .eyes{position: relative;z-index: 3;}
.doraemon .eyes .eye{
  position: absolute;
  top:40rpx;
  width: 72rpx;height: 82rpx;
  background: #fff;
  border: 2rpx solid#000;
  border-radius: 35rpx 35rpx;
}
.doraemon .eyes .eye .black{
  width: 14rpx;height:14rpx;
  background: #000;
  border-radius: 14rpx;
  position: relative;top:40rpx;
}
.doraemon .eyes .left{left:82rpx;}
.doraemon .eyes .right{left:156rpx;}
.doraemon .eyes .eye .bleft{left: 50rpx;}
.doraemon .eyes .eye .bright{left:7rpx;}

.doraemon .face .nose{
  background: #C93300;
  width: 30rpx;height:30rpx;
  border: 2rpx solid #000;
  border-radius: 30rpx;
  position: absolute;
  top:110rpx;left:140rpx;
  z-index: 3;
}
.doraemon .face .nose .light{
  border-radius: 5rpx;
  box-shadow: 19rpx 8rpx 5rpx #fff;
  height: 10rpx;
  width: 10rpx;
}
.doraemon .face .nose_line{
  background: #333;
  width: 3rpx;height:100rpx;
  top: 141rpx;left: 155rpx;
  position: absolute;
  z-index: 3;
}

.doraemon .face .mouth{
  width: 220rpx;height: 400rpx;
  border-bottom: 3rpx solid #333;
  border-radius: 120rpx;
  position: absolute;
  top:-160rpx;left: 45rpx;
}
.doraemon .whiskers{
  background: #fff;
  width: 220rpx;height: 80rpx;
  position: relative;
  top:120rpx;left: 45rpx;
  border-radius: 15rpx;  
  z-index: 2;
}

.doraemon .whiskers .whisker{
  background: #333;
  height: 2rpx;width: 60rpx;
  position: absolute;
  z-index: 2;
}

.doraemon .whiskers .rTop{
  left: 165rpx;top: 25rpx;
}
.doraemon .whiskers .rt{
  left: 167rpx;top: 45rpx;
  height: 4rpx
}
.doraemon .whiskers .rBottom{
  left: 165rpx;top: 65rpx;
}

.doraemon .whiskers .lTop{
  left:0;
  top:25rpx;
}
.doraemon .whiskers .lt{
  left:0;
  top:45rpx;
  height: 4rpx
}
.doraemon .whiskers .lBottom{
  left:0;
  top:65rpx;
}

.doraemon .whiskers .r160{
  transform: rotate(160deg);
  -webkit-transform: rotate(160deg);
}
.doraemon .whiskers .r20{
   transform: rotate(20deg);
  -webkit-transform: rotate(20deg);
}

.doraemon .choker{
  position: relative;
  z-index: 4;
  top: -40rpx;left: 45rpx;
  background: #C40;
  background:-webkit-linear-gradient(linear,left top,left bottom,from(#c40),to(#800400));
  border: 2rpx solid #000000;
  border-radius: 10rpx 10rpx 10rpx 10rpx;
  height: 20rpx;width: 230rpx;
}
.doraemon .choker .bell{
  width: 40rpx;height: 40rpx;_overflow:hidden;
  border-radius: 50rpx;
  border: 2rpx solid #000;
  background: #f9f12a;
  background: -webkit-gradient(linear,left top,left bottom,from(#f9f12a),color-stop(0.5,#e9e11a),to(#a9a100));
  box-shadow: -5rpx 5rpx 10rpx raba(0,0,0,0.25);
  position: relative;
  top:5rpx;left:90rpx;
}
.doraemon .choker .bell_line{
  background: #f9f12a;
  border-radius: 3rpx 3rpx 0rpx 0rpx;
  border: 2rpx solid #333333;
  height: 2rpx;
  width: 36rpx;
  position: relative;top: 10rpx;
}
.doraemon .choker .bell_circle{
  background: #000;
  border-radius: 5rpx;
  height: 10rpx;
  width: 12rpx;
  position: relative;
  top:14rpx;
  left: 14rpx;
}
.doraemon .choker .bell_under{
  background: #000;
  height: 15rpx;
  width: 3rpx;
  left: 18rpx;
  top: 10rpx;
  position: relative
}
.doraemon .choker .bell_light{
  border-radius: 10rpx;
  box-shadow: 19rpx 8rpx 5rpx #fff;
  height: 12rpx;
  width: 12rpx;
  left: 5rpx;
  top:-35rpx;
  position: relative;
  opacity: 0.7;
}

.doraemon .bodys{position: relative;top:-310rpx;}
.doraemon .bodys .body{
  background: #07beea;
  background: -webkit-gradient(linear,right top,left top,from(#07beea));
  border: 2rpx solid #333;
  height: 165rpx;width: 220rpx;
  position: absolute;
  left: 50rpx;
  top:265rpx;
}

.doraemon .bodys .wraps{
  background: #fff;
  background: -webkit-gradient(linear,right top,left bottom,from(#fff),color-stop(0.75,#fff),color-stop(0.83,#eee),color_stop(0.9,#999),color-stop(0.95,#444),to(#000));
  border: 2rpx solid #000;
  border-radius: 85rpx;
  position: absolute;
  height: 170rpx;
  width: 170rpx;
  left: 72rpx;
  top:230rpx;
}

.doraemon .bodys .pocket{
  position: relative;
  width: 130rpx;
  height: 130rpx;
  border-radius: 65rpx;
  background: #fff;
  background: -webkit-gradient(linear,right top,left bottom,from(#fff),color-stop(0.75,#fff),color-stop(0.83,#eee),color-stop(0.9,#999),color-stop(0.95,#444),to(#000));
  border: 2rpx solid #000;
  top:250rpx;
  left: 92rpx;
}
.doraemon .bodys .pocket_mask{
  position: relative;
  width: 134rpx;
  height: 60rpx;
  background: #fff;
  border-bottom: 2rpx solid #000;
  top: 125rpx;
  left: 92rpx;
}
.doraemon .hand_right{
  height: 100rpx;
  width: 100rpx;
  position: absolute;
  top: 272rpx;
  left: 248rpx;
}
.doraemon .hand_left{
  height: 100rpx;
  width: 100rpx;
  position: absolute;
  top:272rpx;
  left: -10rpx;
}
.doraemon .arm{
  background: #07beea;
  background: -webkit-gradient(linear,left top,left bottom,from(#07beea),color-stop(0.85,#07beea),to(#555));
  border: 1rpx solid #000000;
  box-shadow: -10rpx 7rpx 10rpx rgba(0,0,0,0.35);
  height: 50rpx;
  width: 80rpx;
  z-index: -1;
  position: relative;
}

.doraemon .hand_right .arm{
  top:17rpx;
  transform: rotate(35deg);
  -webkit-transform: rotate(35deg);
}
.doraemon .hand_left .arm{
  top:17rpx;
  background: #0096be;
  box-shadow: 5rpx -7rpx 10rpx rgba(0,0,0,0.25);
  transform: rotate(145deg);
  -webkit-transform: rotate(145deg);
}

.doraemon .circle{
  position: absolute;
  width: 60rpx;
  height: 60rpx;
  border-radius: 30rpx;
  border: 2rpx solid #000;
  background: #fff;
  background: -webkit-gradient(linear,right top,left bottom,from(#fff),color-stop(0.5,#fff),color-stop(0.7,#eee),color-stop(0.8,#ddd),to(#999));
}
.doraemon .hand_right .circle{
  left:40rpx;
  top: 32rpx;
}
.doraemon .hand_left .circle{
  left:-20rpx;
  top: 32rpx;
}
.doraemon .arm_rewrite{
  background: #07beea;
  height: 45rpx;
  width: 5rpx;
  position: relative;
}
.doraemon .hand_right .arm_rewrite{
  top: -45rpx;
  left: 22rpx;
}
.doraemon .hand_left .arm_rewrite{
  top:-45rpx;
  left: 60rpx;
  background: #0096be;
}

.doraemon .foot{
  height: 40rpx;
  left: 20rpx;
  position: relative;
  top: -141rpx;
  width: 280rpx;
}
.doraemon .foot .left{
  background: #fff;
  background: -webkit-gradient(linear,right top,left bottom,from(#fff),color-stop(0.75,#fff),color-stop(0.85,#eee),to(#999));
  border: 2rpx solid #333;
  border-radius: 80rpx 60rpx 60rpx 40rpx;
  box-shadow: -6rpx 0 10rpx rgba(0,0,0,0.35);
  height: 30rpx;
  left: 8rpx;
  position: relative;
  top: 65rpx;
  width: 125rpx;
}
.doraemon .foot .right{
  background: #fff;
  background: -webkit-gradient(linear,right top,left bottom,from(#fff),color-stop(0.75,#fff),color-stop(0.85,#eee),to(#999));
  border: 2rpx solid #333;
  border-radius: 80rpx 60rpx 60rpx 40rpx;
  box-shadow: -6rpx 0 10rpx rgba(0,0,0,0.35);
  height: 30rpx;
  left: 141rpx;
  position: relative;
  top: 31rpx;
  width: 125rpx;
}
.doraemon .foot .foot_rewrite{
  position: relative;
  top: -11rpx;
  left: 130rpx;
  _left:127rpx;
  width: 20rpx;
  height: 10rpx;
  background: #fff;
  background: -webkit-gradient(linear,right top,left bottom,from(#666),color-stop(0.83,#fff),to(#fff));
  border-top: 2rpx solid #000;
  border-left: 2rpx solid #000;
  border-right: 2rpx solid #000;
  border-top-right-radius: 40rpx;
  border-top-left-radius: 40rpx;
}
@keyframes eyemove{
  80%{margin: 0;}
  85%{margin: -20rpx 0 0 0;}
  90%{margin: 0 0 0 0;}
  93%{margin: 0 0 0 7rpx;}
  96%{margin: 0 0 0 0;}
}
.doraemon .eyes .eye .black{
  animation-name: eyemove;
  animation-duration: 2s;
  animation-timing-function: linear;
  animation-iteration-count: 20000;
}


