.slide-image{
  width: 100%;
  height: 100%;
}



.searchBar{
  margin-top: 40rpx;
  margin-right: 30rpx;
  margin-left: 30rpx;
  border-top: 1rpx solid rgba(128, 128, 128, 0.192);
  border-bottom: 1rpx solid rgba(128, 128, 128, 0.192);
}

.search{
  font-size: 30rpx;
  color: rgba(128, 128, 128, 0.61);
  font-weight: lighter;
  float: right;
  margin-top: 60rpx;
  position: relative;
}

.search-item{
  /* 
    制作边角为圆形的矩形框
   */
  border: 1px solid #d0d0d0;  
  border-radius: 10rpx;
  text-align: center;
}

.btn{
  width: 70rpx;
  height: 50rpx;
  font-size: 30rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  float: right;
  border: 1px solid #d0d0d0;  
  border-radius: 10rpx;
  margin-left: 15rpx;
}

.title1{
  margin-top: 30rpx;
  font-size: 20px;
  font-style: italic;
  font-weight: bold;
}

.title2{
  font-size: 10px;
  color: rgba(128, 128, 128, 0.877);
}

.chooseWho{
  width: 100%;
  height: 450rpx;
  /* background-color: pink; */
}

.who{
  background-color: rgba(128, 128, 128, 0.062);
  margin-top: 10%;
  margin-left: 10%;
  width: 20%;
  height: 70rpx;
  font-size: 35rpx;
  float: left;
  text-align: center;
  border-left: solid 8rpx rgba(128, 128, 128, 0.363);
  
  /* 
    文字垂直居中
   */
  display: flex;
  align-items: center;
  justify-content: center;
}

.search-icon{
  position: absolute;
  top: 10rpx;
  left: 8rpx;
}

.fish{
  margin-left: 45%;
}

.fish-icon{
  width: 130rpx;
  height: 130rpx;
}