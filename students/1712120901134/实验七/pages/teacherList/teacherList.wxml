<view class="teacher">
	<view wx:for="{{ teacherList }}" wx:key="item.id">
		<view class="teacher_box">
			<image src="{{ item.pic }}" class="teacher_icon"></image>
			<text class="teacher_name">{{ item.name }}</text>
		</view>
		<text class="teacher_info">事迹：{{ item.info }}...</text>
	</view>
</view>